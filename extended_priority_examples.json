            "penalty_cost": 1500.0
          },
          {
            "address": "789 Incomplete Config Customer, New York, NY",
            "priority_level": "high"
          },
          {
            "address": "321 Default Values Customer, New York, NY"
          }
        ]
      }
    },
    {
      "name": "Complete System Configuration",
      "description": "Full configuration with all optional parameters",
      "request": {
        "addresses": [
          "123 Start Street, New York, NY",
          "456 Priority Customer 1, New York, NY",
          "789 Priority Customer 2, New York, NY",
          "321 Regular Customer, New York, NY",
          "654 Time Window Customer, New York, NY",
          "999 End Street, New York, NY"
        ],
        "priority_addresses": [
          {
            "address": "456 Priority Customer 1, New York, NY",
            "priority_level": "critical_high",
