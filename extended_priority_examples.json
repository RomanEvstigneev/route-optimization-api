{
  "examples": [
    {
      "name": "Basic Route (No Priorities)",
      "description": "Simple route optimization without any priority configurations",
      "request": {
        "addresses": [
          "123 Start Street, New York, NY",
          "456 Customer A, New York, NY",
          "789 Customer B, New York, NY", 
          "321 Customer C, New York, NY",
          "999 End Street, New York, NY"
        ]
      }
    },
    {
      "name": "Legacy Priority System (Backwards Compatible)",
      "description": "Using the old priority system format - still works with new validation",
      "request": {
        "addresses": [
          "123 Start Street, New York, NY",
          "456 High Priority Customer, New York, NY",
          "789 Medium Priority Customer, New York, NY",
          "321 Low Priority Customer, New York, NY", 
          "999 End Street, New York, NY"
        ],
        "priority_addresses": [
          {
            "address": "456 High Priority Customer, New York, NY",
            "priority_level": "high",
            "preferred_time_window": "early"
          },
          {
            "address": "789 Medium Priority Customer, New York, NY", 
            "priority_level": "medium",
            "preferred_time_window": "middle"
          },
          {
            "address": "321 Low Priority Customer, New York, NY",
            "priority_level": "low",
            "preferred_time_window": "late"
          }
        ]
      }
    },
    {
      "name": "Extended Priority System (Full Features)",
      "description": "Using all new extended priority levels and time windows",
      "request": {
        "addresses": [
          "123 Start Street, New York, NY",
          "456 Critical Emergency Customer, New York, NY",
          "789 High Priority VIP, New York, NY",
          "321 Standard Customer, New York, NY",
          "654 Low Priority Customer, New York, NY",
          "987 Flexible Timing Customer, New York, NY",
          "999 End Street, New York, NY"
        ],
        "priority_addresses": [
          {
            "address": "456 Critical Emergency Customer, New York, NY",
            "priority_level": "critical_high",
            "preferred_time_window": "earliest",
            "penalty_cost": 2000.0
          },
          {
            "address": "789 High Priority VIP, New York, NY",
            "priority_level": "high", 
            "preferred_time_window": "early"
          },
          {
            "address": "321 Standard Customer, New York, NY",
            "priority_level": "medium",
            "preferred_time_window": "middle"
          },
          {
            "address": "654 Low Priority Customer, New York, NY",
            "priority_level": "low",
            "preferred_time_window": "late"
          },
          {
            "address": "987 Flexible Timing Customer, New York, NY",
            "priority_level": "critical_low",
            "preferred_time_window": "latest",
            "penalty_cost": 5.0
          }
        ],
        "start_time": "2024-12-21T08:00:00Z",
