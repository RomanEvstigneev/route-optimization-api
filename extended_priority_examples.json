      }
    },
    {
      "name": "Mixed Configuration (Partial New Format)",
      "description": "Some addresses with partial configuration - defaults will be applied",
      "request": {
        "addresses": [
          "123 Start Street, New York, NY",
          "456 Critical Customer, New York, NY",
          "789 Incomplete Config Customer, New York, NY",
          "321 Default Values Customer, New York, NY",
          "999 End Street, New York, NY"
        ],
        "priority_addresses": [
          {
            "address": "456 Critical Customer, New York, NY",
            "priority_level": "critical_high",
