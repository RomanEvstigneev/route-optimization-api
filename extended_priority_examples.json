        "time_windows": {
          "enabled": true,
          "windows": [
            {
              "address_index": 4,
              "soft_start_time": "2024-12-21T10:00:00Z",
              "soft_end_time": "2024-12-21T12:00:00Z",
              "cost_per_hour_before": 10.0,
              "cost_per_hour_after": 25.0
            }
          ]
        },
        "start_time": "2024-12-21T08:00:00Z",
        "objective": "minimize_time",
        "service_time_minutes": 4,
        "route_name": "Complete Configuration Example"
      }
    }
  ],
  "error_examples": [
    {
      "name": "Invalid Priority Level",
      "description": "This will fail validation",
      "request": {
        "addresses": ["start", "middle", "end"],
        "priority_addresses": [
          {
            "address": "middle",
            "priority_level": "super_high",
            "preferred_time_window": "early"
          }
        ]
      },
      "expected_error": "Invalid priority_level 'super_high'"
    },
    {
      "name": "Invalid Time Window", 
      "description": "This will fail validation",
      "request": {
        "addresses": ["start", "middle", "end"],
        "priority_addresses": [
          {
            "address": "middle",
            "priority_level": "high",
            "preferred_time_window": "super_early"
          }
        ]
      },
      "expected_error": "Invalid preferred_time_window 'super_early'"
    },
    {
      "name": "Address Not Found",
      "description": "This will fail validation",
      "request": {
        "addresses": ["start", "middle", "end"],
        "priority_addresses": [
          {
            "address": "not_in_list",
            "priority_level": "high", 
            "preferred_time_window": "early"
          }
        ]
      },
      "expected_error": "Priority address 'not_in_list' not found in addresses list"
    },
    {
      "name": "Cannot Prioritize Start Point",
      "description": "This will fail validation",
      "request": {
        "addresses": ["start", "middle", "end"],
        "priority_addresses": [
          {
            "address": "start",
            "priority_level": "high",
            "preferred_time_window": "early"
          }
        ]
      },
      "expected_error": "Cannot set priority for start point"
    },
    {
      "name": "Cannot Prioritize End Point",
      "description": "This will fail validation", 
      "request": {
        "addresses": ["start", "middle", "end"],
        "priority_addresses": [
          {
            "address": "end",
            "priority_level": "high",
            "preferred_time_window": "early"
          }
        ]
      },
      "expected_error": "Cannot set priority for end point"
    },
    {
      "name": "Negative Penalty Cost",
      "description": "This will fail validation",
      "request": {
        "addresses": ["start", "middle", "end"],
        "priority_addresses": [
          {
            "address": "middle",
            "priority_level": "high",
            "preferred_time_window": "early",
            "penalty_cost": -100
          }
        ]
      },
      "expected_error": "Penalty cost must be positive"
    }
  ],
  "api_usage": {
    "endpoint": "POST /api/optimize",
    "content_type": "application/json",
    "curl_example": "curl -X POST http://localhost:8080/api/optimize \\\n  -H 'Content-Type: application/json' \\\n  -d '{\"addresses\": [\"start\", \"middle\", \"end\"]}'"
  },
  "validation_features": {
    "priority_levels": [
      "critical_high",
      "high", 
      "medium",
      "low",
      "critical_low"
    ],
    "time_windows": [
      "earliest",
      "early",
      "middle", 
      "late",
      "latest"
    ],
    "backwards_compatibility": {
      "legacy_priority_levels": ["high", "medium", "low"],
      "legacy_time_windows": ["early", "middle", "late"],
      "note": "Legacy formats are automatically converted to new extended system"
    },
    "validation_version": "2.0",
    "features": [
      "Comprehensive input validation",
      "Clear error messages with suggestions",
      "Backwards compatibility with existing API calls",
      "Extended priority levels and time windows",
      "Custom penalty cost support",
      "Automatic default value application",
      "Duplicate address detection",
      "Start/end point protection"
    ]
  }
}